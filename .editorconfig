root = true

[*]
indent_style = space
end_of_line = crlf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.xml]
indent_size = 2

[*.props]
indent_size = 2

[*.csproj]
indent_size = 2

[*.targets]
indent_size = 2

[*.cs]
csharp_style_namespace_declarations = file_scoped:warning
